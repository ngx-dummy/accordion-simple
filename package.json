{
  "name": "accordion-simple-lib--project",
  "version": "0.3.4",
  "license": "GPL-3.0",
  "description": "A simple accordion component to use in Angular / Ionic projects .. (WIP) and sample app using it",
  "keywords": [
    "accordion",
    "accordion-component",
    "accordion component",
    "accordion-simple",
    "simple accordion",
    "Angular",
    "Angular9",
    "Angular10",
    "Ionic",
    "Ionic5",
    "typescript",
    "javascript",
    "widget",
    "foldable",
    "collapse",
    "expandable",
    "toggle",
    "ui"
  ],
  "private": false,
  "author": {
    "name": "Vladimir Ovsyukov",
    "email": "ovsyukov@yandex.com"
  },
  "engines": {
    "node": ">=12.11.1",
    "npm": ">=6.12.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ngx-dummy/accordion-simple.git"
  },
  "bugs": {
    "email": "ovsyukov@yandex.com",
    "url": "https://github.com/ngx-dummy/accordion-simple/issues"
  },
  "homepage": "https://github.com/ngx-dummy/accordion-simple#README",
  "scripts": {
    "sample": "ng serve accordion-sample -o --host 0.0.0.0 --port 8881 --live-reload",
    "sample:prod": "ng serve accordion-sample -o --host 0.0.0.0 --port 8882 --live-reload --prod",
    "clean:dist": "rimraf ./dist/**",
    "clean:docs": "rimraf ./docs/*",
    "build": "ng b",
    "build:prod": "ng b --prod",
    "build:sample": "ng b accordion-sample",
    "build:sample:prod": "ng b accordion-sample --prod  -- --base-href /accordion-simple/",
    "publish:exec": "sh ./scripts/publish.sh",
    "publish:full": "run-s clean:dist build:prod publish:exec",
    "docs:gh-pages": "sh ./scripts/docs.sh",
    "docs:typedocs": "sh ./scripts/typedocs.sh",
    "docs": "run-s clean:docs docs:gh-pages docs:typedocs",
    "e2e": "ng e2e accordion-sample --webdriverUpdate=true --protractorConfig=projects/accordion-sample/e2e/protractor.conf.js",
    "prepush:origin": "npm run docs",
    "push:origin": "sh ./scripts/push.sh",
    "test:lib": "ng test",
    "test:sample": "ng test accordion-sample",
    "test": "run-p test:lib test:sample",
    "test:ci": "ng test --karma-config=karma-ci.conf.js --no-watch --no-progress",
    "prerelease": "npm run docs",
    "postinstall": "ngcc --tsconfig \"./projects/@ngx-dummy/accordion-simple/tsconfig.lib.json\"",
    "release": "sh ./scripts/release.sh"
  },
  "dependencies": {
    "@angular/animations": "^10.0.9",
    "@angular/common": "^10.0.9",
    "@angular/compiler": "^10.0.9",
    "@angular/core": "^10.0.9",
    "@angular/forms": "^10.0.9",
    "@angular/platform-browser": "^10.0.9",
    "@angular/platform-browser-dynamic": "^10.0.9",
    "@angular/router": "^10.0.9",
    "@material/button": "^7.0.0",
    "@material/card": "^7.0.0",
    "@material/drawer": "^7.0.0",
    "@material/fab": "^7.0.0",
    "@material/layout-grid": "^7.0.0",
    "@material/menu": "^7.0.0",
    "@material/ripple": "^7.0.0",
    "@material/top-app-bar": "^7.0.0",
    "lodash": "^4.17.19",
    "rxjs": "^6.6.2",
    "tslib": "^2.0.1",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.5",
    "@angular-devkit/build-ng-packagr": "^0.1000.5",
    "@angular-eslint/eslint-plugin": "^0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin-template": "^0.0.1-alpha.32",
    "@angular/cli": "^10.0.5",
    "@angular/compiler-cli": "^10.0.9",
    "@angular/language-service": "^10.0.9",
    "@types/jasmine": "^3.5.12",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "chalk": "^4.1.0",
    "codelyzer": "^6.0.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.2.1",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "husky": "^4.2.5",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^10.0.3",
    "node-jq": "^1.11.1",
    "npm-run-all": "^4.1.5",
    "ora": "^5.0.0",
    "prettier": "^2.0.5",
    "protractor": "^7.0.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "simple-git": "^2.17.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typedoc": "^0.18.0",
    "typescript": "~3.9.7"
  }
}
