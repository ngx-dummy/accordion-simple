{
  "name": "accordion-simple-lib--project",
  "version": "0.4.16",
  "license": "GPL-3.0",
  "description": "A simple accordion component to use in Angular / Ionic projects .. (WIP) and sample app using it",
  "keywords": [
    "accordion",
    "accordion-component",
    "accordion component",
    "accordion-simple",
    "simple accordion",
    "Angular",
    "Angular11",
    "Angular12",
    "Ionic",
    "Ionic5",
    "typescript",
    "javascript",
    "widget",
    "foldable",
    "collapse",
    "expandable",
    "toggle",
    "ui"
  ],
  "private": false,
  "author": {
    "name": "Vladimir Ovsyukov",
    "email": "ovsyukov@yandex.com"
  },
  "engines": {
    "node": ">=12.11.1",
    "npm": ">=6.12.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ngx-dummy/accordion-simple.git"
  },
  "bugs": {
    "email": "ovsyukov@yandex.com",
    "url": "https://github.com/ngx-dummy/accordion-simple/issues"
  },
  "homepage": "https://github.com/ngx-dummy/accordion-simple#README",
  "scripts": {
    "clean:docs": "rimraf ./docs/*",
    "build": "ng b",
    "build:prod": "ng b --prod",
    "build:sample": "ng b accordion-sample",
    "build:sample:prod": "ng b accordion-sample --prod  -- --base-href /accordion-simple/",
    "sample": "ng serve accordion-sample -o --host 0.0.0.0 --port 8881 --live-reload",
    "clean:dist": "rimraf ./dist/**",
    "sample:prod": "ng serve accordion-sample -o --host 0.0.0.0 --port 8882 --live-reload --prod",
    "ci:publish:exec": "sh ./scripts/publish.sh",
    "ci:publish:full": "run-s clean:dist build:prod ci:publish:exec",
    "docs:gh-pages": "sh ./scripts/docs.sh",
    "docs:typedocs": "sh ./scripts/typedocs.sh",
    "docs": "run-s clean:docs docs:gh-pages docs:typedocs",
    "e2e": "ng e2e accordion-sample --webdriverUpdate=true --protractorConfig=projects/accordion-sample/e2e/protractor.conf.js",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "prepush:origin": "npm run docs",
    "push:origin": "sh ./scripts/push.sh",
    "test:lib": "ng test",
    "test:sample": "ng test accordion-sample",
    "test": "run-p test:lib test:sample",
    "test:ci": "ng test --karma-config=karma-ci.conf.js --no-watch --progress=false",
    "postinstall": "ngcc --tsconfig \"./projects/@ngx-dummy/accordion-simple/tsconfig.lib.json\"",
    "prerelease": "npm run docs",
    "release": "sh ./scripts/release.sh"
  },
  "dependencies": {
    "@angular/animations": "^11.2.8",
    "@angular/common": "^11.2.8",
    "@angular/compiler": "^11.2.8",
    "@angular/core": "^11.2.8",
    "@angular/forms": "^11.2.8",
    "@angular/platform-browser": "^11.2.8",
    "@angular/platform-browser-dynamic": "^11.2.8",
    "@angular/router": "^11.2.8",
    "@material/button": "^10.0.0",
    "@material/card": "^10.0.0",
    "@material/drawer": "^10.0.0",
    "@material/fab": "^10.0.0",
    "@material/layout-grid": "^10.0.0",
    "@material/menu": "^10.0.0",
    "@material/ripple": "^10.0.0",
    "@material/top-app-bar": "^10.0.0",
    "lodash": "^4.17.21",
    "rxjs": "^6.6.7",
    "tslib": "^2.2.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1102.7",
    "@angular-devkit/build-angular": "^0.1102.7",
    "@angular-devkit/core": "^11.2.7",
    "@angular-eslint/builder": "^2.0.2",
    "@angular-eslint/eslint-plugin": "^2.0.2",
    "@angular-eslint/eslint-plugin-template": "^2.0.2",
    "@angular-eslint/schematics": "^2.0.2",
    "@angular-eslint/template-parser": "^2.0.2",
    "@angular/cli": "^11.2.7",
    "@angular/compiler-cli": "^11.2.8",
    "@angular/language-service": "^11.2.8",
    "@types/jasmine": "^3.6.9",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "chalk": "^4.1.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^11.2.4",
    "node-jq": "^1.12.0",
    "npm-run-all": "^4.1.5",
    "ora": "^5.4.0",
    "prettier": "^2.2.1",
    "protractor": "^7.0.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "simple-git": "^2.37.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.35",
    "typescript": "~4.1.4"
  }
}
